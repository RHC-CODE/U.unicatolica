

{
  "version": 2,
  "builds": [
    {
      "src": "/src/index.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/assets/(.*)",
      "headers": {
        "Content-Type": "application/javascript"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/src/index.html"
    }
  ]
}