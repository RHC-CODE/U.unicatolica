{
  "version": 2,
  "builds": [
    {
      "src": "src/index.html",
      "use": "@vercel/static"
    },
    {
      "src": "src/assets/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/assets/(.*)",
      "dest": "/src/assets/$1",
      "headers": {
        "Content-Type": "text/css"
      }
    },
    {
      "src": "/assets/(.*)",
      "dest": "/src/assets/$1",
      "headers": {
        "Content-Type": "application/javascript"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/src/index.html"
    }
  ]
}